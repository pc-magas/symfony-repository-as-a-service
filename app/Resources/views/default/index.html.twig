{% extends 'base.html.twig' %}

{% block body %}
  <form method="post" action="{{path('add_item')}}">
    <input type="text" name="name"/>
    <button type="submit">Submit</button>
  </form>
  <ul>
      {% for item in items %}
        <li>{{item.name}}</li>
      {% endfor %}
  </ul>
{% endblock %}
